<html>
<SCRIPT TYPE="text/javascript">
function dropdown(current,refenrence)
{
	var myCurrent, myReference;
	myCurrent = current.options[current.selectedIndex].value;
	myReference = refenrence.options[refenrence.selectedIndex].value;
	if(myCurrent == "" || myReference == ""){
		alert("Both options must be filled!");
		return false;
	}

	if(myCurrent == myReference){
		alert("The current and the reference cannot be the same");
		return false;
	}
	else {
		return true;
	}

}
</SCRIPT>

